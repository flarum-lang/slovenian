{
	"name": "flarum-lang/slovenian",
	"type": "flarum-extension",
	"description": "Slovenian language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/slovenian",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"sl-locale",
		"slovenian-locale",
		"slovenian"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/slovenian/issues",
		"source": "https://github.com/flarum-lang/slovenian"
	},
	"require": {
		"flarum/core": "^1.6"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Slovenian",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "sl",
			"title": "Slovenian"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
